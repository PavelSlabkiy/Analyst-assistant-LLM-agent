{
    "id": {
        "field_name": "id",
        "description": "Уникальный идентификатор вакансии",
        "type": "int",
        "sample": 20000
    },
    "analytics_id": {
        "field_name": "analytics_id",
        "description": "Внутренний аналитический идентификатор вакансии",
        "type": "string",
        "sample": "oQj4NLR7"
    },
    "published_at": {
        "field_name": "published_at",
        "description": "Дата публикации вакансии",
        "type": "date",
        "sample": "2024-06-24"
    },
    "is_active": {
        "field_name": "is_active",
        "description": "Признак активности вакансии",
        "type": "bool",
        "sample": "True"
    },
    "position": {
        "field_name": "position",
        "description": "Название должности/вакансии",
        "type": "string",
        "sample": "Senior Data Engineer"
    },
    "specialization": {
        "field_name": "specialization",
        "description": "Профессиональная специализация (на английском)",
        "type": "string",
        "sample": "Data Engineering"
    },
    "position_level": {
        "field_name": "position_level",
        "description": "Уровень позиции",
        "type": "string",
        "sample": "Senior"
    },
    "salary": {
        "field_name": "salary",
        "description": "Значение зарплаты в рублях, может быть NaN",
        "type": "string",
        "sample": "225000"
    },
    "salary_hidden": {
        "field_name": "salary_hidden",
        "description": "Признак скрытой зарплаты",
        "type": "bool",
        "sample": "False"
    },
    "salary_taxes": {
        "field_name": "salary_taxes",
        "description": "Тип указания зарплаты (до или после налогов)",
        "type": "string",
        "sample": "net"
    },
    "salary_is_total": {
        "field_name": "salary_is_total",
        "description": "Признак совокупного дохода",
        "type": "bool",
        "sample": "False"
    },
    "salary_by_our_version": {
        "field_name": "salary_by_our_version",
        "description": "Зарплата рассчитана или уточнена внутренней системой",
        "type": "bool",
        "sample": "True"
    },
    "locations": {
        "field_name": "locations",
        "description": "Набор допустимых форматов работы и регионов",
        "type": "list[string]",
        "sample": ["moscow", "remote"]
    },
    "display_locations": {
        "field_name": "display_locations",
        "description": "Структурированные данные о локациях для отображения",
        "type": "list[object]",
        "sample": [{"city": "Москва", "metro": "м. Киевская", "country": "Россия"}]
    },
    "city": {
        "field_name": "city",
        "description": "Город или список городов размещения вакансии",
        "type": "string",
        "sample": "Москва"
    },
    "country": {
        "field_name": "country",
        "description": "Страна размещения вакансии",
        "type": "string",
        "sample": "Россия"
    },
    "location_details": {
        "field_name": "location_details",
        "description": "Дополнительная информация о локации (метро, офис)",
        "type": "string",
        "sample": "м. Павелецкая"
    },
    "stack": {
        "field_name": "stack",
        "description": "Список технологий и инструментов, может быть пустым",
        "type": "list[string]",
        "sample": ["Python", "SQL", "Kafka"]
    },
    "team_size": {
        "field_name": "team_size",
        "description": "Размер команды",
        "type": "string",
        "sample": "6 - 10"
    }
}